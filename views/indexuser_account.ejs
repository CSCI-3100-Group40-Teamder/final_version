<meta charset="utf-8">
<title>Show User Information</title>
</head>
<style>
	table, th, td {
  		border: 1px solid black;
  		width : 100;
	}
	

</style>
	<body style='background-color: white'>
		
		
		<div class="container-fluid">
			<br>
			 <p style="color:black;font-size:36px;">This will show a list of user</p> 
		
			<% result.forEach(function(item){%>
			<table  class="table table-bordered success"  >
	  			<tr>
	    				<th>user id: </th><th><a href="/user_module/show_your_user_information?user_id=<%= item.user_id %>" target="_blank"><%= item.user_id %></a></th>
	 			</tr>
	  			<tr>
	    				
	    				<th class="info">nickname:</th>
						<td><%= item.nickname %></td>
	
	  			</tr>
	  			<tr>
	    				
	    				<th class="info">last_name:</th>
						<td><%= item.user_last_name%></td>
	
	  			</tr>
	  			<tr>
	    				
	    				<th class="info">first_name:</th>
						<td><%= item.user_first_name %></td>
	
	  			</tr>
			</table>
			<br>
		<% }); %>
		
		<tr>
	    	<form action="/back" method="GET">
			<br><button class="btn btn-primary btn-sm" name="post_id">Back</button> 
			</form>
	  	</tr>
	  	
		</div>
		
		
		
		
	</body>
</html>